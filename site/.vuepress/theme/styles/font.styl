@font-face {
  font-family: 'ariona';
  src:  url('~fonts/ariona.eot?vvyzkr');
  src:  url('~fonts/ariona.eot?vvyzkr#iefix') format('embedded-opentype'),
    url('~fonts/ariona.ttf?vvyzkr') format('truetype'),
    url('data:font/truetype;charset=utf-8;base64,') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'ariona' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

